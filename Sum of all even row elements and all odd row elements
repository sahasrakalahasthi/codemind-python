m,n=map(int,input().split())
a=[]
for i in range(m):  
    r=list(map(int, input().split()))  
    a.append(r) 
e,o=0,0
for i in range(0,m):
    if (i+1)%2!=0:
        for j in range(0,len(a[i])):
            e+=a[i][j]
    else:
        for j in range(0,len(a[i])):
            o+=a[i][j]
print(e,o)
